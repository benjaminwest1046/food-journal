<nav>
    <div class="nav-wrapper">
      <a href="#" class="brand-logo left">DietTracker</a>
      <ul id="nav-mobile" class="right">
        <% if (currentUser) { %>
          <li>Welcome <%= currentUser.firstName %>! </li>
          <li><a href="/entries">My Journal</a></li>
          <li><a href="/logout">Logout</a></li>
        <% } else { %>
          <li><a href="/login">Login</a></li>
          <li><a href="/signup">Signup</a></li>
        <% } %>
      </ul>
    </div>
  </nav>
<div class="flash">
  <% if (typeof message !== 'undefined') { %>
    <% if (typeof message.error !== 'undefined' && message.error.length > 0) { %>
      <div class="card-panel red lighten-2"><%= message.error %></div>
    <% } %>
    <% if (typeof message.info !== 'undefined' && message.info.length > 0) { %>
      <div class="card-panel #F4D03F lighten-2"><%= message.info %></div>
    <% } %>
    <% if (typeof message.success !== 'undefined' && message.success.length > 0) { %>
      <div class="card-panel #049372 lighten-2"><%= message.success %></div>
    <% } %>
  <% } %>
</div>
